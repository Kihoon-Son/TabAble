{
	"name": "TabAble",
	"short_name": "TabAble Research",
	"version": "0.0.1",
	"manifest_version": 3,
	"offline_enabled": true,
	"description": "Project by team 9",
	"author": "stefanXO",
	"icons": {
		"16": "images/browsers16.png",
		"48": "images/browsers48.png",
		"64": "images/browsers64.png",
		"128": "images/browsers128.png",
		"256": "images/browsers256.png"
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"storage",
		"tabGroups",
		"windows"
	],
	"optional_permissions": [
		"system.display"
	],
	"incognito":"spanning",
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {
		"default_icon": {
			"16": "images/browsers16.png",
			"48": "images/browsers48.png",
			"64": "images/browsers64.png",
			"128": "images/browsers128.png",
			"256": "images/browsers256.png"
		}
	},
	"options_ui": {
		"page": "options.html"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+M",
				"windows": "Ctrl+Shift+M",
				"mac": "Command+Shift+M"
			}
		},
		"switch_to_previous_active_tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Space",
				"windows": "Ctrl+Shift+Space",
				"mac": "Command+Shift+Space"
			},
			"description": "Switches to previously active tab"
		}
	}
}